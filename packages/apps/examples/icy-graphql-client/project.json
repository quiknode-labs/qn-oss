{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/apps/examples/icy-graphql-client/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/apps/examples/icy-graphql-client",
        "main": "packages/apps/examples/icy-graphql-client/src/main.ts",
        "tsConfig": "packages/apps/examples/icy-graphql-client/tsconfig.app.json",
        "assets": ["packages/apps/examples/icy-graphql-client/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/apps/examples/icy-graphql-client/src/environments/environment.ts",
              "with": "packages/apps/examples/icy-graphql-client/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "apps-examples-icy-graphql-client:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "apps-examples-icy-graphql-client:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/apps/examples/icy-graphql-client/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/apps/examples/icy-graphql-client"],
      "options": {
        "jestConfig": "packages/apps/examples/icy-graphql-client/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
