{
  "name": "@quicknode/sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/quiknode-labs/qn-oss.git",
    "directory": "packages/libs/sdk"
  },
  "license": "MIT",
  "version": "2.3.0",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "@solana/web3.js": "^1.91",
    "cross-fetch": "^3.1.6",
    "tslib": "^2.5.3",
    "viem": "^2.13.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@pollyjs/adapter-node-http": "^6.0.5",
    "@pollyjs/core": "^6.0.5",
    "@pollyjs/persister-fs": "^6.0.5",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.3",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "scripts": {},
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./index.d.ts"
    },
    "./core": {
      "import": "./esm/core/index.js",
      "default": "./cjs/core/index.js",
      "types": "./esm/core/index.d.ts"
    },
    "./solana": {
      "import": "./esm/solana/index.js",
      "default": "./cjs/solana/index.js",
      "types": "./esm/solana/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "core": [
        "./esm/core/index.d.ts"
      ],
      "solana": [
        "./esm/solana/index.d.ts"
      ]
    }
  }
}
